{ "scope": "meta.export", "settings": { "foreground": "#fca5a5" } },
{ "scope": "meta.function-call", "settings": { "foreground": "#60a5fa" } },
{ "scope": "meta.class", "settings": { "foreground": "#fcd34d" } },
{ "scope": "source.python", "settings": { "foreground": "#e5e7eb" } },
